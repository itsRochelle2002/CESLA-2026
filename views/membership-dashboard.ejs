      /* ‚îÄ‚îÄ‚îÄ APP FORM STATUS ‚îÄ‚îÄ‚îÄ */
      function updateFormStatus(fs) {
        const bar = document.getElementById("formStatusBar");
        const icon = document.getElementById("fsbIcon");
        const ttl = document.getElementById("fsbTitle");
        const dsc = document.getElementById("fsbDesc");
        const btnS = document.getElementById("btnSubmitForm");
        const btnE = document.getElementById("btnEditForm");

        if (fs === "incomplete") {
          bar.className = "form-status-bar incomplete";
          icon.textContent = "üìù";
          ttl.textContent = "Application Form Not Yet Submitted";
          dsc.textContent = "Fill out and submit your application form below.";
          btnS.style.display = "inline-block";
          btnS.disabled = false;
          btnS.textContent = "Submit";
          btnE.style.display = "none";
          setFormLock(false);
          document.getElementById("famTbl").classList.remove("locked");
        } else if (fs === "submitted") {
          bar.className = "form-status-bar submitted";
          icon.textContent = "‚è≥";
          ttl.textContent = "Form Submitted ‚Äî Awaiting Admin Approval";
          dsc.textContent =
            "Your form is under review. You cannot edit it right now.";
          btnS.style.display = "none";
          btnE.style.display = "none";
          setFormLock(true);
          document.getElementById("famTbl").classList.add("locked");
        } else if (fs === "approved") {
          bar.className = "form-status-bar approved";
          icon.textContent = "‚úÖ";
          ttl.textContent = "Application Form Approved!";
          dsc.textContent =
            "Your application is fully approved. This form is now locked.";
          btnS.style.display = "none";
          btnE.style.display = "none";
          setFormLock(true);
          document.getElementById("famTbl").classList.add("locked");
        }
      }
      function setFormLock(locked) {
        const box = document.getElementById("appFormBox");
        if (!box) return;
        box
          .querySelectorAll(
            "input:not([type=radio]):not([type=file]),select,textarea",
          )
          .forEach((el) => {
            if (el.id === "af_date" || el.id === "af_appno") return;
            el.disabled = locked;
          });
        box.querySelectorAll("input[type=radio]").forEach((el) => {
          el.disabled = locked;
        });
        document.querySelectorAll(".emp-tab").forEach((b) => {
          b.style.pointerEvents = locked ? "none" : "auto";
          b.style.opacity = locked ? "0.6" : "1";
        });
        const pb = document.getElementById("afPhotoBox");
        if (pb) pb.style.pointerEvents = locked ? "none" : "auto";
      }

