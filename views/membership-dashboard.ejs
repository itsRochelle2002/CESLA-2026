    <script>
      let D = {};
      let editing = false;

      /* ‚îÄ‚îÄ‚îÄ INIT ‚îÄ‚îÄ‚îÄ */
      async function init() {
        try {
          const r = await fetch("/membership/dashboard/data");
          if (r.status === 401) {
            location.href = "/membership/login";
            return;
          }
          D = await r.json();
          renderDashboard();
          loadFinancials();
          startSlideshow();
        } catch (e) {
          location.href = "/membership/login";
        }
      }

      /* ‚îÄ‚îÄ‚îÄ RENDER ‚îÄ‚îÄ‚îÄ */
      function renderDashboard() {
        const fn =
          [D.first_name, D.middle_name, D.last_name]
            .filter(Boolean)
            .join(" ") || null;
        const uid = D.user_id || D.application_no || "‚Äî";
        const st = D.status || "pending";
        const fs = D.form_status || "incomplete";

        // Top bar
        // Topbar: show first name, or full name, fallback to User ID
        const displayName = D.first_name
          ? D.first_name + (D.last_name ? " " + D.last_name : "")
          : uid;
        document.getElementById("topName").textContent = displayName;

        // Profile hero ‚Äî Firstname Lastname order
        const fullName = D.first_name
          ? D.first_name + (D.last_name ? " " + D.last_name : "")
          : "‚Äî ‚Äî";
        document.getElementById("profName").textContent = fullName;
        document.getElementById("profUid").textContent =
          "USER ID: " + uid.replace(/-/g, " ‚Äì ");

        // Load saved profile photo if exists
        if (D.profile_photo) {
          document.getElementById("profAvaDefault").style.display = "none";
          const img = document.getElementById("profAvaImg");
          img.src = D.profile_photo;
