      /* ‚îÄ‚îÄ‚îÄ MINI TIMELINE ‚îÄ‚îÄ‚îÄ */
      function renderMiniTimeline(st, fs) {
        const steps = [
          {
            l: "Account Registered",
            d: "User ID and password created.",
            done: true,
          },
          {
            l: "Account Approved",
            d:
              st === "approved"
                ? "Account approved."
                : "Waiting for admin approval.",
            done: st === "approved" || st === "rejected",
            curr: st === "pending",
          },
          {
            l: "Application Form",
            d:
              fs === "incomplete"
                ? "Fill out your application form."
                : fs === "submitted"
                  ? "Form under review."
                  : "Form approved!",
            done: fs === "approved",
            curr: st === "approved" && fs === "incomplete",
            sub: fs === "submitted",
          },
          {
            l: "Fully Active",
            d:
              fs === "approved"
                ? "Membership is fully active!"
                : "Pending form approval.",
            done: fs === "approved",
          },
        ];
        const el = document.getElementById("miniTimeline");
        if (!el) return;
        el.innerHTML = steps
          .map(
            (s, i) => `
        <div class="tlm-step">
            <div style="display:flex;flex-direction:column;align-items:center">
                <div class="tlm-dot ${s.done ? "done" : s.curr || s.sub ? "curr" : "wait"}">${s.done ? "‚úì" : s.curr || s.sub ? "‚è≥" : i + 1}</div>
                ${i < steps.length - 1 ? `<div class="tlm-line ${s.done ? "done" : ""}"></div>` : ""}
            </div>
            <div class="tlm-body"><h4>${s.l}</h4><p>${s.d}</p></div>
        </div>`,
          )
          .join("");
      }

