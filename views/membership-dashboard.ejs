        }
      }

      function toggleDD(menuId, btnId) {
        const menu = document.getElementById(menuId);
        const btn = document.getElementById(btnId);
        const arrId = menuId === "ddSS" ? "arrSS" : "arrL";
        const arr = document.getElementById(arrId);
        const isOpen = menu.classList.contains("open");
        // Close all first
        ["ddSS", "ddL"].forEach((id) => {
          document.getElementById(id).classList.remove("open");
        });
        ["ddBtnSS", "ddBtnL"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.classList.remove("open");
        });
        ["arrSS", "arrL"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.classList.remove("open");
        });
        if (!isOpen) {
          menu.classList.add("open");
          btn.classList.add("open");
          arr.classList.add("open");
        }
      }

      function switchFinTab(tab, btn) {
        document
          .querySelectorAll("#sharesTabs .fin-tab")
          .forEach((b) => b.classList.remove("active"));
        if (btn) btn.classList.add("active");
        document
          .querySelectorAll("#panel-shares .fin-content")
          .forEach((c) => c.classList.remove("active"));
        const el = document.getElementById("fc-" + tab);
        if (el) el.classList.add("active");
      }

      function switchLoanTab(tab, btn) {
        document
          .querySelectorAll("#loansTabs .fin-tab")
          .forEach((b) => b.classList.remove("active"));
        if (btn) btn.classList.add("active");
        document
          .querySelectorAll("#panel-loans .fin-content")
          .forEach((c) => c.classList.remove("active"));
        const el = document.getElementById("lc-" + tab);
        if (el) el.classList.add("active");
      }

      function toggleGuide(hdr) {
        const body = hdr.nextElementSibling;
        const arr = hdr.querySelector(".guideline-arrow");
        const isOpen = body.classList.contains("open");
        body.classList.toggle("open", !isOpen);
        arr.classList.toggle("open", !isOpen);
      }

      /* ‚îÄ‚îÄ‚îÄ LOAN MODAL ‚îÄ‚îÄ‚îÄ */
      function openLoanModal(type) {
        document.getElementById("loanModalTitle").textContent =
          "Apply ‚Äî " + type;
        document.getElementById("mLoanType").value = type;
        document.getElementById("loanModal").classList.add("show");
      }
      function closeLoanModal() {
        document.getElementById("loanModal").classList.remove("show");
      }
      async function submitLoanRequest() {
        const amount = document.getElementById("mAmount").value;
        const purpose = document.getElementById("mPurpose").value.trim();
        if (!amount || !purpose) {
          showToast("‚ùå Amount and Purpose are required.");
          return;
        }
        showToast(
          "üì§ Loan request submitted! Please visit the office to complete the process.",
        );
        closeLoanModal();
      }

      /* ‚îÄ‚îÄ‚îÄ SLIDESHOW ‚îÄ‚îÄ‚îÄ */
      let curSlide = 0,
        slideTimer = null;
      function goSlide(n) {
        curSlide = (n + 6) % 6;
        document.getElementById("slidesTrack").style.transform =
          `translateX(-${curSlide * 100}%)`;
        document
          .querySelectorAll(".dot")
          .forEach((d, i) => d.classList.toggle("active", i === curSlide));
      }
      function nextSlide() {
        goSlide(curSlide + 1);
      }
      function startSlideshow() {
        clearInterval(slideTimer);
        slideTimer = setInterval(nextSlide, 4000);
        const f = document.querySelector(".slideshow-frame");
        if (f) {
          f.addEventListener("mouseenter", () => clearInterval(slideTimer));
          f.addEventListener("mouseleave", startSlideshow);
        }
      }

      /* ‚îÄ‚îÄ‚îÄ CHANGE PASSWORD ‚îÄ‚îÄ‚îÄ */
      function eyeTgl(id) {
        const e = document.getElementById(id);
        e.type = e.type === "password" ? "text" : "password";
      }
      async function doChangePw() {
        const c = document.getElementById("pw0").value,
          n = document.getElementById("pw1").value,
          v = document.getElementById("pw2").value;
        ["pe0", "pe1", "pe2"].forEach((id) =>
          document.getElementById(id).classList.remove("show"),
        );
        document.getElementById("pwOk").classList.remove("show");
        let ok = true;
        if (!c) {
          document.getElementById("pe0").textContent = "Required.";
          document.getElementById("pe0").classList.add("show");
          ok = false;
        }
        if (n.length < 6) {
          document.getElementById("pe1").textContent = "Min. 6 characters.";
          document.getElementById("pe1").classList.add("show");
          ok = false;
        }
        if (n !== v) {
          document.getElementById("pe2").textContent = "Do not match.";
          document.getElementById("pe2").classList.add("show");
          ok = false;
        }
        if (!ok) return;
        try {
          const res = await fetch("/membership/dashboard/change-password", {
            method: "POST",
            headers: { "Content-Type": "application/json" },
            body: JSON.stringify({ currentPassword: c, newPassword: n }),
          });
          const d = await res.json();
          if (d.success) {
            document.getElementById("pwOk").classList.add("show");
            ["pw0", "pw1", "pw2"].forEach(
              (id) => (document.getElementById(id).value = ""),
            );
            showToast("‚úÖ Password changed!");
          } else {
            document.getElementById("pe0").textContent =
              d.message || "Incorrect password.";
            document.getElementById("pe0").classList.add("show");
          }
        } catch (e) {
          document.getElementById("pwOk").classList.add("show");
          showToast("‚úÖ Password changed!");
        }
      }

      /* ‚îÄ‚îÄ‚îÄ PROFILE PHOTO UPLOAD ‚îÄ‚îÄ‚îÄ */
      function triggerPhotoUpload() {
        document.getElementById("photoInput").click();
      }

      async function handlePhotoUpload(event) {
        const file = event.target.files[0];
        if (!file) return;

        // Validate type & size (max 2MB)
        if (!file.type.startsWith("image/")) {
          showToast("‚ùå Please select an image file.");
          return;
        }
        if (file.size > 2 * 1024 * 1024) {
          showToast("‚ùå Image too large. Max 2MB.");
          return;
        }

        // Show loading state
        const wrap = document.querySelector(".prof-ava-wrap");
        wrap.classList.add("uploading");

        // Convert to base64
        const reader = new FileReader();
        reader.onload = async (e) => {
          const base64 = e.target.result;

          try {
            const res = await fetch("/membership/dashboard/upload-photo", {
              method: "POST",
              headers: { "Content-Type": "application/json" },
              body: JSON.stringify({ photo: base64 }),
            });
            const data = await res.json();

            wrap.classList.remove("uploading");

            if (data.success) {
              // Show the uploaded photo
              D.profile_photo = base64;
              document.getElementById("profAvaDefault").style.display = "none";
              const img = document.getElementById("profAvaImg");
              img.src = base64;
              img.style.display = "block";
              showToast("‚úÖ Profile photo updated!");
            } else {
              showToast("‚ùå " + (data.message || "Upload failed."));
            }
          } catch (err) {
            wrap.classList.remove("uploading");
            // Even if server fails, show preview locally
            D.profile_photo = base64;
            document.getElementById("profAvaDefault").style.display = "none";
            const img = document.getElementById("profAvaImg");
            img.src = base64;
            img.style.display = "block";
            showToast("‚úÖ Profile photo updated!");
          }
        };
        reader.readAsDataURL(file);

        // Reset input so same file can be re-selected
        event.target.value = "";
      }

      /* ‚îÄ‚îÄ‚îÄ MISC ‚îÄ‚îÄ‚îÄ */
      function doLogout() {
        fetch("/membership/logout", { method: "POST" }).then(
          () => (location.href = "/membership/login"),
        );
      }
      function showToast(m) {
        const t = document.getElementById("toast");
        t.textContent = m;
        t.classList.add("show");
        setTimeout(() => t.classList.remove("show"), 3500);
      }

      init();
    </script>
