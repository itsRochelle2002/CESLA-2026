      async function submitAppForm() {
        const btn = document.getElementById("btnSubmitForm");
        if (
          !document.getElementById("af_ln").value.trim() ||
          !document.getElementById("af_fn").value.trim() ||
          !document.getElementById("af_dob").value ||
          !document.getElementById("af_pa").value.trim()
        ) {
          showToast("‚ùå Fill in all required (*) fields.");
          return;
        }
        const fam = [];
        for (let i = 0; i < 4; i++) {
          const n = document
            .querySelector(`input[data-f="name"][data-r="${i}"]`)
            ?.value.trim();
          if (n)
            fam.push({
              name: n,
              relation:
                document.querySelector(
                  `input[data-f="relation"][data-r="${i}"]`,
                )?.value || "",
              age:
                document.querySelector(`input[data-f="age"][data-r="${i}"]`)
                  ?.value || "",
              occupation:
                document.querySelector(
                  `input[data-f="occupation"][data-r="${i}"]`,
                )?.value || "",
            });
        }
        btn.disabled = true;
        btn.textContent = "Submitting...";
        try {
          const csRadio = document.querySelector('input[name="af_cs"]:checked');
          const natRadio = document.querySelector(
            'input[name="af_nat_r"]:checked',
          );
          const payload = {
            lastName: document.getElementById("af_ln").value.trim(),
            firstName: document.getElementById("af_fn").value.trim(),
            middleName: document.getElementById("af_mn").value.trim(),
            suffix: document.getElementById("af_sx").value.trim(),
            birthdate: document.getElementById("af_dob").value,
            placeOfBirth: document.getElementById("af_pob").value.trim(),
            gender:
              document.querySelector('input[name="afg"]:checked')?.value || "",
            civilStatus: csRadio?.value || "",
            nationality:
              natRadio?.value === "Others"
                ? document.getElementById("af_nat").value
                : "Filipino",
            religion: document.getElementById("af_rel").value,
            dependents: document.getElementById("af_dep").value || 0,
            sssNumber: document.getElementById("af_sss").value,
            tinNumber: document.getElementById("af_tin").value,
            presentAddress: document.getElementById("af_pa").value.trim(),
            zipCode1: document.getElementById("af_z1").value,
            permanentAddress: document.getElementById("af_pma").value,
            stayYears: document.getElementById("af_sy").value || 0,
            referredBy: document.getElementById("af_ref").value,
            // Employment
            employmentType:
              document
                .querySelector(".emp-tab.active")
                ?.textContent?.toLowerCase()
                .trim() || "",
            position: document.getElementById("af_pos")?.value || "",
            employerName: document.getElementById("af_emp")?.value || "",
            monthlyIncome: document.getElementById("af_inc")?.value || 0,
            familyMembers: fam,
          };
          const res = await fetch("/membership/dashboard/submit-form", {
            method: "POST",
            headers: { "Content-Type": "application/json" },
            body: JSON.stringify(payload),
          });
          const data = await res.json();
          if (data.success) {
            D.form_status = "submitted";
            Object.assign(D, {
              last_name: payload.lastName,
              first_name: payload.firstName,
              middle_name: payload.middleName,
            });
            updateFormStatus("submitted");
            renderMiniTimeline(D.status, "submitted");
            renderDashboard();
            showToast("‚úÖ Application form submitted!");
          } else {
            btn.disabled = false;
            btn.textContent = "Submit";
            showToast("‚ùå " + (data.message || "Submission failed."));
          }
        } catch (e) {
          btn.disabled = false;
          btn.textContent = "Submit";
          showToast("‚ùå Server error. Please try again.");
        }
      }

      /* ‚îÄ‚îÄ‚îÄ FINANCIALS ‚îÄ‚îÄ‚îÄ */
      async function loadFinancials() {
        try {
          const [sr, svr, lr] = await Promise.all([
            fetch("/membership/dashboard/shares"),
            fetch("/membership/dashboard/savings"),
            fetch("/membership/dashboard/loans"),
          ]);
          const sh = await sr.json(),
            sv = await svr.json(),
            lo = await lr.json();
          const f = (n) =>
            parseFloat(n || 0).toLocaleString("en-PH", {
              minimumFractionDigits: 2,
            });
          const fd = (d) => (d ? new Date(d).toLocaleDateString("en-PH") : "‚Äî");
          const ts = sh.length ? parseFloat(sh[0].balance || 0) : 0;
          const tv = sv.length ? parseFloat(sv[0].balance || 0) : 0;
          const tl = lo
            .filter((l) => l.status === "active")
            .reduce((a, l) => a + parseFloat(l.outstanding_balance || 0), 0);
          const al = lo.filter((l) => l.status === "active").length;

          // Overview slides
          const es = document.getElementById("ovShares");
          if (es) es.textContent = "‚Ç± " + f(ts);
          const ev = document.getElementById("ovSavings");
          if (ev) ev.textContent = "‚Ç± " + f(tv);
          const el = document.getElementById("ovLoan");
          if (el) el.textContent = "‚Ç± " + f(tl);
          const eln = document.getElementById("ovLoanSub");
          if (eln)
            eln.textContent = al + " Active Loan" + (al === 1 ? "" : "s");

          // Savings
          document.getElementById("totalSavings").textContent = "‚Ç± " + f(tv);
          document.getElementById("savingsTxnCount").textContent =
            sv.length + " transactions";
          document.getElementById("savingsTbody").innerHTML = sv.length
            ? sv
                .map(
                  (t) =>
                    `<tr><td>${fd(t.transaction_date)}</td><td><span class="tag ${t.type}">${t.type.toUpperCase()}</span></td><td>${t.description || "‚Äî"}</td><td>${t.or_number || "‚Äî"}</td><td class="${t.type === "deposit" ? "pos" : "neg"}">${t.type === "deposit" ? "+" : "-"}‚Ç±${f(t.amount)}</td><td style="font-weight:700">‚Ç±${f(t.balance)}</td></tr>`,
                )
                .join("")
            : `<tr><td colspan="6" class="empty-td">No savings transactions yet.</td></tr>`;

          // Shares
          document.getElementById("totalShares").textContent = "‚Ç± " + f(ts);
          document.getElementById("sharesTxnCount").textContent =
            sh.length + " transactions";
          document.getElementById("sharesTbody").innerHTML = sh.length
            ? sh
                .map(
                  (t) =>
                    `<tr><td>${fd(t.transaction_date)}</td><td><span class="tag ${t.type}">${t.type.toUpperCase()}</span></td><td>${t.description || "‚Äî"}</td><td>${t.or_number || "‚Äî"}</td><td class="${t.type === "deposit" ? "pos" : "neg"}">${t.type === "deposit" ? "+" : "-"}‚Ç±${f(t.amount)}</td><td style="font-weight:700">‚Ç±${f(t.balance)}</td></tr>`,
                )
                .join("")
            : `<tr><td colspan="6" class="empty-td">No share transactions yet.</td></tr>`;

          // Loans
          document.getElementById("loanOutstanding").textContent = "‚Ç± " + f(tl);
          document.getElementById("loanActiveSub").textContent =
            al + " active loan" + (al === 1 ? "" : "s");
          document.getElementById("loansTbody").innerHTML = lo.length
            ? lo
                .map(
                  (l) =>
                    `<tr><td><strong>${l.loan_no}</strong></td><td>${(l.loan_type || "").toUpperCase()}</td><td>‚Ç±${f(l.amount)}</td><td>‚Ç±${f(l.monthly_payment)}</td><td class="neg">‚Ç±${f(l.outstanding_balance)}</td><td>${fd(l.due_date)}</td><td><span class="tag ${l.status || "active"}">${(l.status || "").toUpperCase()}</span></td></tr>`,
                )
                .join("")
            : `<tr><td colspan="7" class="empty-td">No loan records yet.</td></tr>`;
        } catch (e) {}
      }

