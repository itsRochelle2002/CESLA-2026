<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Membership Application Form - CESLA</title>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;600;700;800&display=swap" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js"></script>
    <style>
        * { margin:0; padding:0; box-sizing:border-box; }
        body { font-family:"Montserrat",sans-serif; background:var(--cesla-navy); min-height:100vh; display:flex; flex-direction:column; }

        .top-bar {
            background:linear-gradient(135deg,#0d1b3e 0%,#0d1b3e 100%);
            margin:16px 20px 0;
            border-radius:14px;
            padding:14px 24px;
            display:flex;
            align-items:center;
            justify-content:space-between;
            box-shadow:0 6px 24px rgba(0,0,0,0.25);
            gap:12px;
        }
        .back-btn { font-size:1.4em; color:#1a1a2e; text-decoration:none; font-weight:800; }
        .top-bar h1 { font-size:1.1em; font-weight:800; color:#1a1a2e; flex:1; text-align:center; }
        .top-actions { display:flex; gap:8px; }
        .btn-top {
            background:#1a1a2e; color:#fff; border:none; border-radius:8px;
            padding:8px 14px; font-family:"Montserrat",sans-serif; font-size:0.75em;
            font-weight:700; cursor:pointer; transition:background 0.2s;
        }
        .btn-top:hover { background:var(--cesla-navy); }

        .page-wrapper { flex:1; display:flex; justify-content:center; padding:28px 16px 40px; }

        #printableForm {
            background:#fff;
            width:100%;
            max-width:820px;
            border-radius:12px;
            box-shadow:0 16px 48px rgba(0,0,0,0.3);
            padding:32px 36px;
        }

        /* FORM HEADER */
        .form-header { display:flex; align-items:center; gap:20px; margin-bottom:8px; padding-bottom:12px; border-bottom:2px solid #1a1a2e; }
        .form-header img.logo { width:90px; height:90px; object-fit:contain; border-radius:50%; }
        .header-info { flex:1; }
        .header-info h1 { font-size:1.3em; font-weight:800; color:#1a1a2e; }
        .header-info p { font-size:0.78em; color:#444; margin-top:3px; }
        .photo-box { width:80px; height:90px; border:2px solid #1a1a2e; display:flex; align-items:center; justify-content:center; font-size:0.7em; color:#777; text-align:center; font-weight:600; flex-shrink:0; }

        .form-title { text-align:center; font-size:1em; font-weight:800; color:#1a1a2e; margin:14px 0 10px; letter-spacing:1px; text-decoration:underline; }

        /* TABLE */
        .form-table { width:100%; border-collapse:collapse; font-size:0.78em; }
        .form-table td, .form-table th { border:1px solid #1a1a2e; padding:5px 8px; vertical-align:top; }
        .form-table th { background:#f0f0f0; font-weight:800; text-align:center; font-size:0.85em; letter-spacing:0.5px; }
        .section-header { background:#e8e8e8; font-weight:800; text-align:center; letter-spacing:1px; font-size:0.85em; padding:6px; }
        label.field-label { font-size:0.85em; font-weight:600; color:#333; }
        input[type="text"],input[type="date"],input[type="number"],input[type="email"],input[type="tel"] {
            border:none; border-bottom:1px solid #333; width:100%;
            font-family:"Montserrat",sans-serif; font-size:0.9em; padding:2px 4px; outline:none; background:transparent;
        }
        input[type="radio"],input[type="checkbox"] { margin-right:3px; accent-color:#2a3272; }
        .radio-row { display:flex; flex-wrap:wrap; gap:10px; align-items:center; }
        .radio-row label { font-size:0.9em; font-weight:600; cursor:pointer; }

        /* DECLARATION */
        .declaration { border:1px solid #1a1a2e; padding:10px 12px; font-size:0.72em; line-height:1.6; color:#333; margin-top:4px; }

        /* SIGNATURES */
        .sig-row { display:flex; gap:20px; margin-top:16px; }
        .sig-box { flex:1; text-align:center; }
        .sig-line { border-top:1px solid #333; margin-bottom:4px; margin-top:36px; }
        .sig-box label { font-size:0.72em; font-weight:600; }

        /* SUBMIT SECTION */
        .submit-section {
            margin-top:24px;
            padding:20px 24px;
            background:#f0f4ff;
            border:2px dashed #2a3272;
            border-radius:12px;
        }
        .submit-section h3 { font-size:0.88em; font-weight:800; color:#2a3272; margin-bottom:14px; }

        /* Account summary box */
        .account-summary {
            background:var(--cesla-navy);
            color:#fff;
            border-radius:10px;
            padding:12px 18px;
            font-size:0.8em;
            font-weight:600;
            margin-bottom:16px;
            display:flex;
            align-items:center;
            gap:10px;
        }
        .account-summary span { opacity:0.8; }
        .account-summary strong { color:#f5c518; }

        .btn-submit-form {
            width:100%;
            padding:15px;
            background:#28a745;
            color:#fff;
            font-family:"Montserrat",sans-serif;
            font-size:0.95em;
            font-weight:800;
            letter-spacing:1px;
            border:none;
            border-radius:10px;
            cursor:pointer;
            transition:background 0.2s, transform 0.2s;
        }
        .btn-submit-form:hover { background:#218838; transform:translateY(-2px); }

        /* SUCCESS MODAL */
        .overlay { display:none; position:fixed; inset:0; background:rgba(0,0,0,0.5); z-index:200; align-items:center; justify-content:center; }
        .overlay.active { display:flex; }
        .modal-card {
            background:#fff; border-radius:18px; padding:36px 28px; max-width:380px; width:90%;
            text-align:center; display:flex; flex-direction:column; align-items:center; gap:14px;
            box-shadow:0 20px 60px rgba(0,0,0,0.4); animation:popIn 0.25s ease;
        }
        @keyframes popIn { from{opacity:0;transform:scale(0.9)} to{opacity:1;transform:scale(1)} }
        .modal-icon { font-size:3em; }
        .modal-card h3 { font-size:1.1em; font-weight:800; color:#1a1a2e; }
        .modal-card p { font-size:0.82em; color:#555; line-height:1.6; font-weight:500; }
        .modal-card .username-display { font-weight:800; color:#2a3272; font-size:1em; }
        .btn-modal-ok {
            background:var(--cesla-navy); color:#fff; font-family:"Montserrat",sans-serif;
            font-size:0.85em; font-weight:700; padding:12px 32px;
            border:none; border-radius:10px; cursor:pointer; transition:background 0.2s;
        }
        .btn-modal-ok:hover { background:#1a1a5e; }

        @media print {
            body { background:white; }
            .top-bar, .submit-section, .no-print { display:none !important; }
            .page-wrapper { padding:0; }
            #printableForm { box-shadow:none; border-radius:0; padding:16px; }
        }
        @media(max-width:600px){
            #printableForm { padding:16px 12px; }
            .top-bar { margin:10px 10px 0; padding:12px 14px; }
            .top-bar h1 { font-size:0.85em; }
            .form-header img.logo { width:65px; height:65px; }
        }
    </style>
    <link rel="stylesheet" href="/css/global.css" />
</head>
<body>

    <!-- Top Bar -->
    <div class="top-bar no-print">
        <a href="/membership/register" class="back-btn">&#8592;</a>
        <h1>Membership Application Form</h1>
        <div class="top-actions">
            <button class="btn-top" onclick="window.print()">üñ®Ô∏è Print</button>
            <button class="btn-top" onclick="savePDF()">üìÑ Save PDF</button>
        </div>
    </div>

    <div class="page-wrapper">
        <div id="printableForm">

            <!-- Form Header -->
            <div class="form-header">
                <img class="logo" src="/images/CESLA_logo.png" alt="CESLA Logo" />
                <div class="header-info">
                    <h1>CESLA Multi-Purpose Cooperative</h1>
                    <p>CLIMBS Bldg., Upper Zone 5, National Highway, Bulua, Cagayan De Oro City</p>
                    <p>Email: cec@climbs.coop</p>
                </div>
                <div class="photo-box">2x2<br>Photo</div>
            </div>

            <div class="form-title">APPLICATION FORM</div>

            <table class="form-table">
                <tr>
                    <td style="width:50%"><label class="field-label">Application Date:</label> <input type="date" id="applicationDate" /></td>
                    <td style="width:50%"><label class="field-label">Application No:</label> <input type="text" id="applicationNo" readonly /></td>
                </tr>

                <!-- PERSONAL DETAILS -->
                <tr><td colspan="2" class="section-header">PERSONAL DETAILS</td></tr>
                <tr>
                    <td colspan="2">
                        <div class="radio-row">
                            <label><input type="radio" name="title" value="Mr"> Mr.</label>
                            <label><input type="radio" name="title" value="Mrs"> Mrs.</label>
                            <label><input type="radio" name="title" value="Ms"> Ms.</label>
                            &nbsp;&nbsp;
                            <strong>Gender:</strong>
                            <label><input type="radio" name="gender" value="Male"> Male</label>
                            <label><input type="radio" name="gender" value="Female"> Female</label>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td><label class="field-label">Last Name *</label><input type="text" id="lastName" required /></td>
                    <td><label class="field-label">First Name *</label><input type="text" id="firstName" required /></td>
                </tr>
                <tr>
                    <td><label class="field-label">Middle Name</label><input type="text" id="middleName" /></td>
                    <td><label class="field-label">Suffix</label><input type="text" id="suffix" style="width:100px" /></td>
                </tr>
                <tr>
                    <td><label class="field-label">Date of Birth (MM/DD/YYYY) *</label><input type="date" id="birthdate" required /></td>
                    <td><label class="field-label">Place of Birth *</label><input type="text" id="placeOfBirth" required /></td>
                </tr>
                <tr>
                    <td>
                        <label class="field-label">Nationality:</label><br>
                        <div class="radio-row">
                            <label><input type="radio" name="nationality" value="Filipino"> Filipino</label>
                            <label><input type="radio" name="nationality" value="Others"> Others:</label>
                            <input type="text" id="nationalityOther" style="width:120px" />
                        </div>
                    </td>
                    <td>
                        <label class="field-label">Religion:</label><input type="text" id="religion" /><br><br>
                        <label class="field-label">Number of Dependents:</label><input type="number" id="dependents" min="0" style="width:80px" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <label class="field-label">Civil Status:</label>
                        <div class="radio-row">
                            <label><input type="radio" name="civilStatus" value="Single"> Single</label>
                            <label><input type="radio" name="civilStatus" value="Married"> Married</label>
                            <label><input type="radio" name="civilStatus" value="Legally Separated"> Legally Separated</label>
                            <label><input type="radio" name="civilStatus" value="Others"> Others</label>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td><label class="field-label">SSS/GSIS Number:</label><input type="text" id="sssNumber" /></td>
                    <td><label class="field-label">Tax Identification Number:</label><input type="text" id="tinNumber" /></td>
                </tr>
                <tr>
                    <td><label class="field-label">Other IDs:</label><input type="text" id="otherIds" /></td>
                    <td>
                        <label class="field-label">Who recommended you to CEC?</label><input type="text" id="referredBy" /><br><br>
                        <label class="field-label">Contact No:</label><input type="tel" id="referredContact" />
                    </td>
                </tr>

                <!-- FAMILY MEMBERS -->
                <tr><td colspan="2" class="section-header">FAMILY MEMBERS</td></tr>
                <tr>
                    <td colspan="2" style="padding:0">
                        <table style="width:100%;border-collapse:collapse;font-size:0.9em;">
                            <tr>
                                <th style="border:1px solid #1a1a2e;padding:5px;background:#f0f0f0;">Name</th>
                                <th style="border:1px solid #1a1a2e;padding:5px;background:#f0f0f0;">Relation</th>
                                <th style="border:1px solid #1a1a2e;padding:5px;background:#f0f0f0;width:60px">Age</th>
                                <th style="border:1px solid #1a1a2e;padding:5px;background:#f0f0f0;">Occupation</th>
                            </tr>
                            <tr>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f1n"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f1r"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="number" id="f1a" min="0"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f1o"></td>
                            </tr>
                            <tr>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f2n"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f2r"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="number" id="f2a" min="0"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f2o"></td>
                            </tr>
                            <tr>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f3n"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f3r"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="number" id="f3a" min="0"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f3o"></td>
                            </tr>
                            <tr>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f4n"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f4r"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="number" id="f4a" min="0"></td>
                                <td style="border:1px solid #1a1a2e;padding:4px"><input type="text" id="f4o"></td>
                            </tr>
                        </table>
                    </td>
                </tr>

                <!-- CONTACT DETAILS -->
                <tr><td colspan="2" class="section-header">CONTACT DETAILS</td></tr>
                <tr>
                    <td><label class="field-label">Present Address: *</label><input type="text" id="presentAddress" required /></td>
                    <td><label class="field-label">Zip Code:</label><input type="text" id="zipCode1" style="width:100px" /></td>
                </tr>
                <tr>
                    <td colspan="2">
                        <label class="field-label">Length of Stay in Present Address:</label>
                        <input type="number" id="stayYears" min="0" style="width:70px" placeholder="years" /> years
                        <input type="number" id="stayMonths" min="0" max="11" style="width:70px" placeholder="months" /> months
                    </td>
                </tr>
                <tr>
                    <td><label class="field-label">Permanent Address:</label><input type="text" id="permanentAddress" /></td>
                    <td><label class="field-label">Zip Code:</label><input type="text" id="zipCode2" style="width:100px" /></td>
                </tr>

                <!-- EMPLOYMENT DETAILS -->
                <tr><td colspan="2" class="section-header">EMPLOYMENT DETAILS</td></tr>
                <tr>
                    <td colspan="2" style="padding:0">
                        <table style="width:100%;border-collapse:collapse;font-size:0.9em;">
                            <tr>
                                <th style="border:1px solid #1a1a2e;padding:6px;background:#f0f0f0;width:34%">EMPLOYED</th>
                                <th style="border:1px solid #1a1a2e;padding:6px;background:#f0f0f0;width:33%">SELF-EMPLOYED</th>
                                <th style="border:1px solid #1a1a2e;padding:6px;background:#f0f0f0;width:33%">UNEMPLOYED</th>
                            </tr>
                            <tr>
                                <td style="border:1px solid #1a1a2e;padding:6px;vertical-align:top">
                                    <label class="field-label">Employer/Business Name:</label><input type="text" id="employerName" /><br><br>
                                    <label class="field-label">Office Address:</label><input type="text" id="officeAddress" /><br><br>
                                    <label class="field-label">Nature of Business:</label><input type="text" id="businessNature" /><br><br>
                                    <label class="field-label">Office No:</label><input type="tel" id="officeNo" style="width:90px" />
                                    <label class="field-label">Fax No:</label><input type="tel" id="faxNo" style="width:80px" /><br><br>
                                    <label class="field-label">Employment Type:</label><br>
                                    <div class="radio-row">
                                        <label><input type="checkbox" name="empType" value="Private"> Private</label>
                                        <label><input type="checkbox" name="empType" value="Government"> Government</label>
                                        <label><input type="checkbox" name="empType" value="Others"> Others</label>
                                    </div><br>
                                    <label class="field-label">Position/Rank:</label><input type="text" id="position" /><br><br>
                                    <label class="field-label">Monthly Income:</label><input type="number" id="monthlyIncome" min="0" /><br><br>
                                    <span style="font-size:0.8em;font-style:italic">*If less than 6 months, fill-up below:</span><br><br>
                                    <label class="field-label">Previous Employer:</label><input type="text" id="prevEmployer" /><br><br>
                                    <label class="field-label">Yrs. in Company:</label><input type="text" id="prevYears" /><br><br>
                                    <label class="field-label">Position/Rank:</label><input type="text" id="prevPosition" />
                                </td>
                                <td style="border:1px solid #1a1a2e;padding:6px;vertical-align:top">
                                    <label class="field-label">Name of Business:</label><input type="text" id="businessName" /><br><br>
                                    <label class="field-label">Type of Business:</label><br>
                                    <div class="radio-row">
                                        <label><input type="checkbox" name="bizType" value="Sole Prop"> Sole Prop</label>
                                        <label><input type="checkbox" name="bizType" value="Partnership"> Partnership</label>
                                        <label><input type="checkbox" name="bizType" value="Corp"> Corp</label>
                                    </div><br>
                                    <label class="field-label">Nature of Business:</label><input type="text" id="selfBusinessNature" /><br><br>
                                    <label class="field-label">Asset Size (Php):</label><input type="number" id="assetSize" min="0" /><br><br>
                                    <label class="field-label">Share in Business (%):</label><input type="number" id="businessShare" min="0" max="100" /><br><br>
                                    <label class="field-label">Monthly Income:</label><input type="number" id="selfMonthlyIncome" min="0" />
                                </td>
                                <td style="border:1px solid #1a1a2e;padding:6px;vertical-align:top">
                                    <div style="display:flex;flex-direction:column;gap:12px">
                                        <label><input type="checkbox" name="unemployedType" value="Housewife"> Housewife</label>
                                        <label><input type="checkbox" name="unemployedType" value="Student"> Student</label>
                                        <label><input type="checkbox" name="unemployedType" value="Others"> Others (please specify)</label>
                                        <input type="text" id="unemployedOther" />
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>

            <!-- DECLARATION -->
            <div class="declaration" style="margin-top:8px">
                I/We hereby certify that all the data and statements in this application are correct and are made for obtaining credit, and the signature(s) appearing thereon is(are) genuine. I/We authorize you to obtain such information as you may require concerning the statements made in this application and that the sources which you may apply are authorized to provide any information relative to this application. I/We agree this will remain your property whether the credit is granted or not.
            </div>

            <!-- SIGNATURES -->
            <div class="sig-row">
                <div class="sig-box"><div class="sig-line"></div><label>Signature of Applicant over Printed Name</label></div>
                <div class="sig-box"><div class="sig-line"></div><label>Signature of Spouse over Printed Name</label></div>
                <div class="sig-box"><div class="sig-line"></div><label>Witnessed by</label></div>
            </div>

            <!-- SUBMIT SECTION (no-print) -->
            <div class="submit-section no-print">
                <h3>üìã ACCOUNT REGISTRATION SUMMARY</h3>
                <!-- Shows the username from register page -->
                <div class="account-summary">
                    <span>üë§ Account Username:</span>
                    <strong id="displayUsername">‚Äî</strong>
                    <span style="margin-left:auto; font-size:0.85em; opacity:0.7">Pending Admin Verification</span>
                </div>
                <button class="btn-submit-form" onclick="submitApplication()">
                    üíæ Submit Application
                </button>
            </div>

        </div>
    </div>

    <!-- SUCCESS MODAL -->
    <div class="overlay" id="successOverlay">
        <div class="modal-card">
            <div class="modal-icon">‚úÖ</div>
            <h3>Application Submitted!</h3>
            <p>
                Your account <span class="username-display" id="modalUsername"></span> and membership application have been submitted successfully.<br><br>
                <strong>Your account is now pending verification.</strong> An administrator will review your application and activate your account. You will be notified once approved.
            </p>
            <button class="btn-modal-ok" onclick="window.location='/'">Back to Home</button>
        </div>
    </div>

    <script>
        // Auto generate application number
        document.getElementById('applicationNo').value = 'APP-' + Date.now().toString().slice(-6);

        // Read username saved from register page
        const savedUsername = sessionStorage.getItem('reg_username') || '';
        const savedPassword = sessionStorage.getItem('reg_password') || '';
        if (savedUsername) {
            document.getElementById('displayUsername').textContent = savedUsername;
        } else {
            // If user navigated directly without registering, redirect back
            // Uncomment below line if you want to enforce registration first:
            // window.location.href = '/membership/register';
        }

        function savePDF() {
            const el = document.getElementById('printableForm');
            const noPrint = el.querySelectorAll('.no-print');
            noPrint.forEach(n => n.style.display = 'none');
            const opt = {
                margin: 8,
                filename: 'CESLA-MemberApplication-' + new Date().toISOString().slice(0,10) + '.pdf',
                image: { type: 'jpeg', quality: 0.98 },
                html2canvas: { scale: 2 },
                jsPDF: { unit: 'mm', format: 'a4', orientation: 'portrait' }
            };
            html2pdf().set(opt).from(el).save().then(() => {
                noPrint.forEach(n => n.style.display = '');
            });
        }

        function submitApplication() {
            // Validate required fields
            const required = [
                { id: 'lastName',       label: 'Last Name' },
                { id: 'firstName',      label: 'First Name' },
                { id: 'birthdate',      label: 'Date of Birth' },
                { id: 'placeOfBirth',   label: 'Place of Birth' },
                { id: 'presentAddress', label: 'Present Address' },
            ];

            let missing = [];
            required.forEach(f => {
                const el = document.getElementById(f.id);
                if (!el.value.trim()) {
                    el.style.borderBottom = '2px solid #e74c3c';
                    missing.push(f.label);
                } else {
                    el.style.borderBottom = '1px solid #333';
                }
            });

            if (!savedUsername) {
                alert('No account found. Please go back and create your account first.');
                window.location.href = '/membership/register';
                return;
            }

            if (missing.length > 0) {
                alert('Please fill in the required fields:\n‚Ä¢ ' + missing.join('\n‚Ä¢ '));
                return;
            }

            // Collect all form data + credentials
            const applicationData = {
                // Account credentials from register page
                username: savedUsername,
                password: savedPassword,
                status: 'pending', // needs admin verification

                // Personal details
                title:          document.querySelector('input[name="title"]:checked')?.value || '',
                gender:         document.querySelector('input[name="gender"]:checked')?.value || '',
                lastName:       document.getElementById('lastName').value,
                firstName:      document.getElementById('firstName').value,
                middleName:     document.getElementById('middleName').value,
                suffix:         document.getElementById('suffix').value,
                birthdate:      document.getElementById('birthdate').value,
                placeOfBirth:   document.getElementById('placeOfBirth').value,
                nationality:    document.querySelector('input[name="nationality"]:checked')?.value || '',
                religion:       document.getElementById('religion').value,
                dependents:     document.getElementById('dependents').value,
                civilStatus:    document.querySelector('input[name="civilStatus"]:checked')?.value || '',
                sssNumber:      document.getElementById('sssNumber').value,
                tinNumber:      document.getElementById('tinNumber').value,
                presentAddress: document.getElementById('presentAddress').value,
                applicationNo:  document.getElementById('applicationNo').value,
                submittedAt:    new Date().toISOString(),
            };

            // POST to server
            fetch('/membership/register/submit', {
                method: 'POST',
                headers: { 'Content-Type': 'application/json' },
                body: JSON.stringify(applicationData)
            })
            .then(res => res.json())
            .then(data => {
                if (data.success) {
                    // Clear session storage
                    sessionStorage.removeItem('reg_username');
                    sessionStorage.removeItem('reg_password');
                    // Show success modal
                    document.getElementById('modalUsername').textContent = '"' + applicationData.username + '"';
                    document.getElementById('successOverlay').classList.add('active');
                } else {
                    alert('Submission failed: ' + (data.message || 'Unknown error'));
                }
            })
            .catch(() => {
                // If no backend yet, just show success modal for UI demo
                document.getElementById('modalUsername').textContent = '"' + applicationData.username + '"';
                document.getElementById('successOverlay').classList.add('active');
                sessionStorage.removeItem('reg_username');
                sessionStorage.removeItem('reg_password');
            });
        }
    </script>
</body>
</html>
